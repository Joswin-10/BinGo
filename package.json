{
  "name": "bingo",
  "private": true,
  "scripts": {
    "dev": "npm run dev --prefix frontend",
    "dev:backend": "cd backend && python -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev --prefix frontend\"",
    "build": "npm run build --prefix frontend",
    "preview": "npm run preview --prefix frontend",
    "lint": "npm run lint --prefix frontend"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
